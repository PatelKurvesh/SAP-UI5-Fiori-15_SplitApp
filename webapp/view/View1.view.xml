<mvc:View controllerName="GithubSAP-UI5-Fiori-15_SplitApp.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<List items="{/Company}" itemPress="onItemPress">
						<ObjectListItem
						type="Navigation"
						title="{name}"
						icon="{logo}"
						>
							
						</ObjectListItem>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>